<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.hannatyden.cykelfest.MyLocationDemoActivity">


    <TextView
        android:id="@+id/info"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/img"
        android:clickable="true"
        android:gravity="center"
        android:text="info"
        android:textColor="@android:color/holo_green_dark"
        android:translationZ="5dp"
        android:background="@drawable/common_google_signin_btn_text_light_normal_background"
        android:textSize="20sp"
        app:layout_constraintTop_toTopOf="parent"

        />

    <fragment
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="0dp"
        tools:context="com.example.hannatyden.cykelfest.MyLocationDemoActivity"
        tools:layout_editor_absoluteY="100dp"
        tools:layout_width="match_parent" />

    <ImageButton
        android:id="@+id/bikeButton"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:src="@drawable/bad"
        android:layout_gravity="bottom" /><![CDATA[





    android:layout_width="368dp"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="8dp">



]]>
</FrameLayout>


