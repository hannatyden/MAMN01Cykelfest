<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.hannatyden.cykelfest.MyLocationDemoActivity">


    <TextView
        android:id="@+id/info"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/img"
        android:background="@drawable/common_google_signin_btn_text_light_normal_background"
        android:clickable="true"
        android:text="Du är inte på festens destination!"
        android:textColor="@android:color/holo_green_dark"
        android:textSize="20sp"
        android:translationZ="5dp"
        app:layout_constraintTop_toTopOf="parent"
        />


    <ImageButton
        android:id="@+id/bikeButton"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_gravity="bottom"
        android:alpha="0"
        android:src="@drawable/bad"
        android:visibility="visible" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="47dp"
        android:layout_height="48dp"
        android:src="@drawable/swipedownv4"
        android:translationZ="10dp"
        android:background="@null"
        android:onClick="arrowButtonPressed"
        android:scaleType="fitCenter"/>

    <fragment
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="0dp"
        tools:context="com.example.hannatyden.cykelfest.MyLocationDemoActivity"
        tools:layout_editor_absoluteY="100dp"
        tools:layout_width="match_parent" /><![CDATA[




    android:layout_width="368dp"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="8dp">




]]>
</FrameLayout>


