<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.hannatyden.cykelfest.MyLocationDemoActivity">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <TextView
            android:id="@+id/info"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:background="@drawable/common_google_signin_btn_text_light_normal_background"
            android:clickable="true"
            android:text="Du är inte på festens destination!"
            android:textColor="@android:color/holo_green_dark"
            android:textSize="20sp"
            android:translationZ="5dp"
            app:layout_constraintTop_toTopOf="parent"
            android:onClick="animateView"/>


        <ImageButton
            android:id="@+id/bikeButton"
            android:layout_width="20dp"
            android:layout_height="18dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentStart="true"
            android:layout_gravity="bottom"
            android:alpha="0"
            android:onClick="testButton"

            android:src="@drawable/bad"
            android:translationZ="7dp"
            android:visibility="visible" />

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="47dp"
            android:layout_height="48dp"
            android:background="@null"
            android:onClick="animateView"
            android:scaleType="fitCenter"
            android:src="@drawable/swipedownv4"
            android:translationZ="10dp" />

        <fragment
            android:id="@+id/map"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            tools:context="com.example.hannatyden.cykelfest.MyLocationDemoActivity"
            tools:layout_editor_absoluteY="100dp"
            tools:layout_width="match_parent" />

        <TextView
            android:id="@+id/destInfo"
            android:layout_width="331dp"
            android:layout_height="305dp"
            android:layout_below="@+id/info"
            android:layout_centerHorizontal="true"
            android:background="@drawable/common_google_signin_btn_text_light_normal_background"
            android:text="Du har kommit till festens destination!"
            android:textColor="@android:color/holo_green_dark"
            android:textSize="20dp"
            android:translationZ="10dp">


        </TextView>

        <Button
            android:id="@+id/goToRanking"
            android:layout_width="252dp"
            android:layout_height="73dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="96dp"
            android:background="@drawable/button_border"
            android:onClick="goToRankingButton"
            android:text="Gå till ranking ->"
            android:translationZ="15dp" /><![CDATA[




    android:layout_width="368dp"

        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="8dp">




    ]]>
    </RelativeLayout>

</FrameLayout>


